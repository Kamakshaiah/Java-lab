package simulations;

import java.util.Random;

public class NumericSims {
	public static double simulateVector(int n) {
		//double[] arr = new double[n];
		
		Random rand = new Random();
		double[] arr = new double[n];
		
		for (int i = 0; i < n; i++) {
			 arr[i] = rand.nextDouble();
		}
		return arr; 
	}
	
	
}
